{
  "name": "template-wordpress-wordmove",
  "version": "1.0.0",
  "author": "FURAZOA",
  "license": "GPL-2.0-or-later",
  "dependencies": {
    "gsap": "^3.12.4",
    "jquery": "^3.7.1",
    "prettier": "^3.2.2",
    "throttle-debounce": "^5.0.0"
  },
  "scripts": {
    "mix:dev": "mix",
    "dev:watch": "mix watch",
    "mix:production": "mix --production",
    "browser:watch": "browser-sync start --config bs-config.js",
    "dev": "npm-run-all -p *:dev",
    "watch": "npm-run-all -p *:watch",
    "build": "npm-run-all *:production",
    "clean": "rimraf ./public",
    "login:wordmove": "docker exec -w /home/wordmove -it [.envで設定したPROJECT_NAME]_wordmove /bin/bash",
    "docker-rebuild": "docker-compose build --no-cache",
    "prestart": "docker-compose up -d",
    "start": "npm run watch",
    "stop": "docker-compose stop"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "argv": "^0.0.2",
    "babel-preset-env": "^1.7.0",
