    "css-mqpacker": "^7.0.0",
    "csswring": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "fs-extra": "^11.2.0",
    "laravel-mix": "^6.0.49",
    "laravel-mix-eslint": "^0.2.0",
    "npm-run-all": "^4.1.5",
